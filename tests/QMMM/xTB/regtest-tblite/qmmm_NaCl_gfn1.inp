&GLOBAL
  PRINT_LEVEL MEDIUM
  PROJECT dat_qmmm_NaCl_gfn1
  RUN_TYPE MD
&END GLOBAL

&MOTION
  &MD
    ENSEMBLE NVE
    STEPS 2
    TEMPERATURE 298
    TIMESTEP 0.5
  &END MD
&END MOTION

&FORCE_EVAL
  METHOD QMMM
  &DFT
    BASIS_SET_FILE_NAME GTH_BASIS_SETS
    CHARGE 0
    POTENTIAL_FILE_NAME ../FAKE_POTENTIAL
    &MGRID
      COMMENSURATE
      CUTOFF 400
    &END MGRID
    &QS
      METHOD xTB
    &END QS
    &SCF
      EPS_SCF 1.E-6
      SCF_GUESS atomic
    &END SCF
  &END DFT
  &MM
    &FORCEFIELD
      &CHARGE
        ATOM NA
        CHARGE 1.0000
      &END CHARGE
      &CHARGE
        ATOM CL
        CHARGE -1.0000
      &END CHARGE
      &NONBONDED
        &LENNARD-JONES
          ATOMS NA NA
          EPSILON 0.0
          RCUT 5.0
          SIGMA 0.0
        &END LENNARD-JONES
        &LENNARD-JONES
          ATOMS CL NA
          EPSILON 0.0
          RCUT 5.0
          SIGMA 0.0
        &END LENNARD-JONES
        &LENNARD-JONES
          ATOMS CL CL
          EPSILON 0.0
          RCUT 5.0
          SIGMA 0.0
        &END LENNARD-JONES
      &END NONBONDED
    &END FORCEFIELD
    &POISSON
      &EWALD
        ALPHA .44
        EWALD_TYPE ewald
        GMAX 41
      &END EWALD
    &END POISSON
  &END MM
  &QMMM
    ECOUPL COULOMB 
    MM_POTENTIAL_FILE_NAME ../MM_POTENTIAL
    NOCOMPATIBILITY
    &CELL
      ABC 17.320500 17.320500 17.320500
    &END CELL
    &INTERPOLATOR
      EPS_R 1.0e-14
      EPS_X 1.0e-14
      MAXITER 100
    &END INTERPOLATOR
    &MM_KIND NA
      RADIUS 1.5875316249000
    &END MM_KIND
    &MM_KIND CL
      RADIUS 1.5875316249000
    &END MM_KIND
    #&PERIODIC
    #  GMAX 0.5
    #  &MULTIPOLE
    #    ANALYTICAL_GTERM
    #    EWALD_PRECISION 0.00000001
    #    RCUT 8.0
    #  &END MULTIPOLE
    #&END PERIODIC
    #&PRINT
    #  &PERIODIC_INFO
    #  &END PERIODIC_INFO
    #  &POTENTIAL
    #  &END POTENTIAL
    #&END PRINT
    &QM_KIND NA
      MM_INDEX 3
    &END QM_KIND
    &QM_KIND CL
      MM_INDEX 2
    &END QM_KIND
    #
    # QM_KINDS
    #
    #
    #
  &END QMMM
  &SUBSYS
    &CELL
      ABC 17.320500 17.320500 17.320500
    &END CELL
    &COORD
      NA        -8.660250   -8.660250   -8.660250
      CL         0.000000    0.000000    0.000000
      NA        -8.660250    0.000000    0.000000
      CL         0.000000    8.660250    8.660250
      NA         0.000000   -8.660250    0.000000
      CL         8.660250    0.000000    8.660250
      NA         0.000000    0.000000   -8.660250
      CL         8.660250    8.660250    0.000000
    &END COORD
    &KIND NA
      BASIS_SET SZV-GTH
      POTENTIAL GTH-P-NOEL
    &END KIND
    &KIND CL
      BASIS_SET SZV-GTH
      POTENTIAL GTH-M-NOEL
    &END KIND
    &TOPOLOGY
    &END TOPOLOGY
  &END SUBSYS
&END FORCE_EVAL
